Config {
    font = "xft:Fantasque Sans Mono:pixelsize=15",
    bgColor = "#000000",
    fgColor = "#DDDDDD",
    commands = [
        Run Date "%H:%M" "date" 600,
        Run BatteryP ["BAT0"] ["-t", "<acstatus>", "-S", "True", "-l", "red", "-n", "yellow", "-h", "green", "--", "-f", "ADP0/online", "-o", "-<left>", "-O", "+<left>"] 600,
        Run StdinReader
    ],
    template = "%StdinReader% }{ <fc=#6294CF>%date%</fc>"
}
