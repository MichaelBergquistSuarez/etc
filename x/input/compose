# XCompose Specification.

include "%L"

# Greek Alphabet

<Multi_key> <g> <a> : "α" U003B1 # GREEK SMALL LETTER ALPHA
<Multi_key> <g> <b> : "β" U003B2 # GREEK SMALL LETTER BETA
<Multi_key> <g> <c> : "γ" U003B3 # GREEK SMALL LETTER GAMMA
<Multi_key> <g> <d> : "δ" U003B4 # GREEK SMALL LETTER DELTA
